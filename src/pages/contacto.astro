---
import BaseLayout from '../layouts/BaseLayout.astro';
import LogoCloud from '../components/LogoCloud.astro';
import Contactoprovisional from '../components/contactoprovisional.astro';


const tabs = {
  tab1: { text: 'Tanque', image: './images/tanque2.jpg' },
  tab2: { text: 'Caja Thermo', image: './images/thermo.jpg' },
  tab3: { text: 'Caja Seca', image: './images/seca.jpg' },
};
---

<BaseLayout>
	<LogoCloud 
   backgroundImage="./images/svgs/wave.svg"
  title=""
  height="h-[55vh]"
  origin='bottom'/>
	<Contactoprovisional
    titleStyle="contact-title"
  textStyle="contact-text"
  infoStyle="contact-info"
  />
</BaseLayout>
